{
  "name": "@trinitiventures/hapi-cognito-auth",
  "version": "2.0.3",
  "description": "A hapi plugin which adds Cognito authentication to your server",
  "author": "Andre Gallo <andre@triniti.net> (https://www.triniti.net)",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "lab",
    "test:debug": "lab --inspect --inspect-brk",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:trinitiventures/hapi-cognito-auth.git"
  },
  "keywords": [
    "hapi",
    "auth",
    "jwt",
    "cognito"
  ],
  "dependencies": {
    "@hapi/boom": "9.1.4",
    "@hapi/hoek": "9.3.0",
    "@hapi/wreck": "17.2.0",
    "@hapipal/haute-couture": "4.3.1",
    "cookie": "0.5.0",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5"
  },
  "devDependencies": {
    "@hapi/code": "8.x.x",
    "@hapi/lab": "24.x.x",
    "eslint": "7.x.x",
    "eslint-config-hapi": "12.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "husky": "4.x.x",
    "lint-staged": "10.x.x"
  },
  "files": [
    "lib/",
    "package.json"
  ],
  "lint-staged": {
    "*.js": [
      "eslint . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
